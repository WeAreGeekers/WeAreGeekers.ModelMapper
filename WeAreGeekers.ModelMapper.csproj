<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <Authors>Fabrizio Pairone</Authors>
    <Company>WeAreGeekers</Company>
    <Version>0.0.1-beta1</Version>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Description>.NetCore project for simplify mapping between complex objects</Description>
    <Copyright>WeAreGeekers</Copyright>
    <PackageLicenseFile>LICENSE.md</PackageLicenseFile>
    <PackageProjectUrl>https://github.com/WeAreGeekers/WeAreGeekers.ModelMapper</PackageProjectUrl>
    <RepositoryUrl>https://github.com/WeAreGeekers/WeAreGeekers.ModelMapper</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>csharp;.NetCore;ModelMapper</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.DependencyInjection.Abstractions" Version="5.0.0" />
  </ItemGroup>

  <ItemGroup>
    <None Include="LICENSE.md">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
